#include<stdio.h>
//wzglednie pierwsze suma liczb

unsigned int NWD(unsigned int a, unsigned int b)
{
    while(a != b)
        if(a > b)
            a = a-b;
        else
            b = b-a;
    return a;
}
unsigned int suma(unsigned int n)
{
    int i, suma = 0;
    for(i = 2; i<n; i++)
    {
        if(NWD(i,n) == 1)
            suma +=i;
    }
    return suma;
}


int main()
{
    int n;
    printf("Podaj liczbe calkowita n: ");
    scanf("%d", &n);
    printf("wynik obliczen to: %d\n", suma(n));
    return 0;

}


#include<stdio.h>

void zlicz(int x)
{
    static int ile=0;
    ile= ile + x;
    printf("f zostalla wywolana %d razy\n ", ile);
}

int main()
{
    zlicz(5);
    zlicz(7);
    zlicz(8);
    zlicz(4);
    zlicz(9);
    zlicz(2);
        return 0;
}

#include<stdio.h>

void zlicz(int x)
{
    static int ile=0;
    ile++;
    printf("f zostalla wywolana %d razy\n ", ile);
}

int main()
{
    zlicz();
    zlicz();
    zlicz();
    zlicz();
    zlicz();
    zlicz();
        return 0;
}

#include<stdio.h>

int wyraz(int n)
{
    if(n==0)
        return 1;
    return 2*wyraz(n-1) +5;

}


int main()
{
    int x;
    scanf("%d", &x);
    printf("a %d = %d", x, wyraz(x));
    return 0;

}

#include<stdio.h>

int wyraz(int n)
{
    if(n==0)
        return 1;
    return 2*wyraz(n-1) +5;

}


int main()
{
    int x;
    for(x=0; x<10; x++)
    printf(" %d ", wyraz(x));
    return 0;

}

#include<stdio.h>

int wyraz(int n)
{
    if(n<2)
        return 1;
    return wyraz(n-1) +2*wyraz(n-2)+3;

}


int main()
{
    int x;
    for(x=0; x<10; x++)
    printf(" %d ", wyraz(x));
    return 0;

}
#include<stdio.h>

int wyraz(int n)
{
    if(n<2)
        return 1;
    return wyraz(n-1) +wyraz(n-2);

}


int main()
{
    int x;
    for(x=0; x<10; x++)
    printf(" %d ", wyraz(x));
    return 0;

}
#include<stdio.h>

int wyraz(int n)
{
    if(n<2)
        return 1;
    return wyraz(n-1) +wyraz(n-2);

}


int main()
{
    int x;
    for(x=0; x<10; x++)
    printf(" %d ", wyraz(x));
    return 0;

}
fibonaczi

#include<stdio.h>
#include<math.h>
int wyraz(int n)
{
    if(n<2)
        return 1;
    return wyraz(n-1) +wyraz(n-2);

}

void ilorazy(int n)
{
    int i;
    for(i=0; i<n; i++)
    {
        printf("a_%d/a_ %d= %f\n", i+1, i, (float)(wyraz(i+1))/wyraz(i));

    }

}

int main()
{
    ilorazy(20);
    return 0;
}
